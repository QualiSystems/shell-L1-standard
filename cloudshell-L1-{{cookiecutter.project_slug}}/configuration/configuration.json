{
  "common_variable": {
    "driver_name": "{{cookiecutter.project_name}}",
    "driver_module": ["{{cookiecutter.project_slug}}.driver_handler", "{{cookiecutter.project_name}}DriverHandler"],
    "connection_type": "tcp",
    "connection_port": 53058,
    "device_prompt": "=>",
    "server_timeout": 10,
    "runtime_configuration": "{{cookiecutter.project_slug}}_runtime_configuration.json"
  },

  "cli_variable": {
    "ssh": ["common.cli.ssh_session", "SSHSession"],
    "telnet": ["common.cli.telnet_session", "TelnetSession"],
    "tcp": ["common.cli.tcp_session", "TCPSession"]
  },

  "driver_variable": {
    "resource_name": [
      "Switch {0}",
      "Blade {0}",
      "Port {0}"
    ],
    "resource_family_name": [
      "{{cookiecutter.family_name}}",
      "{{cookiecutter.family_name}} Blade",
      "{{cookiecutter.family_name}} Port"
    ],
    "resource_model_name": [
      "{{cookiecutter.model_name}}",
      "{{cookiecutter.model_name}} Blade",
      "{{cookiecutter.model_name}} Port"
    ]
  }
}